{
  "name": "@uno/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve --projects=desktop,desktop-web target=start",
    "lint": "nx run-many --target=lint --projects=desktop,desktop-web -- --ignore-pattern .gitignore --ignore-pattern 'node_modules/**' --ignore-pattern 'dist/**' --ignore-pattern '.next/**' --ignore-pattern 'coverage/**'",
    "test": "nx run-many --target=test --projects=desktop,desktop-web",
    "build": "nx run-many --target=build --projects=desktop-web && nx run desktop:build",
    "build:desktop": "nx run desktop-web:build --configuration=production && nx run desktop:build --configuration=production",
    "start:desktop": "nx run-many --target=serve --projects=desktop,desktop-web --parallel",
    "package:desktop": "nx run desktop:build --configuration=production && nx run desktop-web:build --configuration=production && nx run desktop:package"
  },
  "private": true,
  "dependencies": {
    "@fastify/autoload": "~6.0.3",
    "@fastify/sensible": "~6.0.2",
    "axios": "^1.6.0",
